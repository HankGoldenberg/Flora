			    <label for="formGroupExampleInput2">Subject</label>
			    <input name = "subject" type="text" class="form-control" id="formGroupExampleInput2" placeholder="Another input">
			  </div>
			  <div class="form-group">
			    <label for="formGroupExampleInput2">Message</label>
			    <input name = "message" type="text" class="form-control" id="formGroupExampleInput2" placeholder="Another input">
